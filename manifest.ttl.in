@prefix lv2:      <http://lv2plug.in/ns/lv2core#> .
@prefix owl:			<http://www.w3.org/2002/07/owl#> .
@prefix rdfs:     <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:       <http://lv2plug.in/ns/extensions/ui#> .
@prefix kx:				<http://kxstudio.sf.net/ns/lv2ext/external-ui#> .

@prefix eteroj:		<http://open-music-kontrollers.ch/lv2/eteroj#> .

# to please sord_validate
ui:EoUI
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf ui:UI .
kx:Widget
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf ui:UI .
kx:Host
	a lv2:Feature .

# Eteroj IO 
eteroj:io
	a lv2:Plugin ;
	lv2:binary <eteroj@LIB_EXT@> ;
	@EO_UI_WRAP@ui:ui eteroj:io_eo ;
	@UI_UI_WRAP@ui:ui eteroj:io_ui ;
	@KX_UI_WRAP@ui:ui eteroj:io_kx ;
	@X11_UI_WRAP@ui:ui eteroj:io_x11 ;
	rdfs:seeAlso <eteroj.ttl> .

eteroj:io_eo
	a ui:EoUI ;
	ui:binary <eteroj_ui@LIB_EXT@> ;
	rdfs:seeAlso <eteroj.ttl> .
eteroj:io_ui
	a ui:UI ;
	ui:binary <eteroj_ui@LIB_EXT@> ;
	rdfs:seeAlso <eteroj.ttl> .
eteroj:io_kx
	a kx:Widget ;
	ui:binary <eteroj_ui@LIB_EXT@> ;
	rdfs:seeAlso <eteroj.ttl> .
eteroj:io_x11
	a ui:X11UI ;
	ui:binary <eteroj_ui@LIB_EXT@> ;
	rdfs:seeAlso <eteroj.ttl> .
